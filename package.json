{
  "name": "vue3-ts-screen-demo",
  "version": "1.9.0",
  "description": "使用vue3.0 + typescript + ant-design-vue3.0 搭建的项目基础架构类型声明库",
  "author": {
    "name": "zhangshenshen",
    "email": "zhangshenshen@cethik.com"
  },
  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack-dev-server -c build/webpack.dev.js  --progress --color",
    "build": "cross-env NODE_ENV=production ASSET_PATH=screen webpack -c build/webpack.prod.js --progress --color",
    "analy": "cross-env NODE_ENV=production webpack -c build/webpack.analy.js",
    "delete": "rimraf node_modules",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,js,ts,tsx}\" --fix",
    "lint:ls-lint": "ls-lint",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{ts,js,json,tsx,css,less,scss,vue,html,md}\"",
    "commit": "git add . && git-cz",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=16",
    "webpack": ">=5"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@plugin-web-update-notification/webpack": "^1.6.6",
    "@vueuse/core": "^10.3.0",
    "ant-design-vue": "4.x",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "lockr": "^0.8.5",
    "nprogress": "^0.2.0",
    "pinia": "2.1.6",
    "process": "^0.11.10",
    "qs": "^6.11.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-typescript": "^7.24.4",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@ls-lint/ls-lint": "^2.0.1",
    "@types/lockr": "^0.8.8",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vue/compiler-sfc": "^3.3.4",
    "babel-loader": "^9.1.2",
    "commitizen": "^4.3.0",
    "commitlint": "^17.7.1",
    "compression-webpack-plugin": "10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-webpack-plugin": "^4.0.1",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "lint-staged": "^14.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "speed-measure-webpack-plugin": "^1.5.0",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.3",
    "svg-sprite-loader": "^6.0.11",
    "terser-webpack-plugin": "^5.3.9",
    "thread-loader": "^4.0.2",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.16.6",
    "vue-loader": "^17.2.2",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*.{vue,js,ts,tsx}": [
      "npm run lint:eslint"
    ],
    "src/**/*.{js,json,tsx,css,less,scss,vue,html,md}": [
      "npm run lint:prettier",
      "npm run lint:ls-lint"
    ]
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not dead"
  ]
}
